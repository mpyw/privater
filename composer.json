{
    "name": "mpyw/privator",
    "description": "Traits for testing private methods and properties on PHP 7.0+",
    "type": "library",
    "license": "MIT",
    "keywords": ["test", "private"],
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "codeception/codeception": "*",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "mpyw\\Privator\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "php vendor/bin/codecept run unit --coverage --coverage-xml"
        ]
    },
    "archive": {
        "exclude": [
            "*",
            ".*",
            "!/src/*"
        ]
    }
}
